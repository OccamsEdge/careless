year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)
bibentry(bibtype = "Manual",
	 title = "careless: Procedures for computing indices of careless responding",
	 authors = c(person(c("Richard", "Dean"), "Yentes"),
	 						 person("Francisco", "Wilhelm")),
	 year = year,
	 note=note,
	 url = "https://github.com/ryentes/careless")
textVersion = paste0("Yentes R.D., & Wilhelm, F. (", year, "). careless: Procedures for computing indices of careless responding.", note, ".")
